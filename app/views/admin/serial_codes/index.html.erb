<h1>Listing admin_serial_codes</h1>

<table>
  <tr>
    <th>Code</th>
    <th>Person</th>
    <th>Used time</th>
    <th>Batch</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @admin_serial_codes.each do |admin_serial_code| %>
  <tr>
    <td><%= admin_serial_code.code %></td>
    <td><%= admin_serial_code.person %></td>
    <td><%= admin_serial_code.used_time %></td>
    <td><%= admin_serial_code.batch %></td>
    <td><%= link_to 'Show', admin_serial_code %></td>
    <td><%= link_to 'Edit', edit_admin_serial_code_path(admin_serial_code) %></td>
    <td><%= link_to 'Destroy', admin_serial_code, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

