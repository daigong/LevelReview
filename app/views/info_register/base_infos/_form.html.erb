<%= semantic_form_for @info_register_base_info,:validate=>true do |f| %>
  <%= f.inputs do %>
    <%= f.input :nation %>
    <%= f.input :sex %>
    <%= f.input :birthday %>
    <%= f.input :last_review_time %>
    <%= f.input :invite_time %>
    <%= f.input :degree %>
    <%= f.input :edu_background %>
    <%= f.input :current_level %>
    <%= f.input :advance_level %>
    <%= f.input :review_type %>
    <%= f.input :computer_exam %>
    <%= f.input :computer_exam_file_name %>
    <%= f.input :foreign_exam %>
    <%= f.input :foreign_exam_file_name %>
  <% end %>

  <%= f.buttons do %>
    <%= f.commit_button %>
  <% end %>
<% end %>
